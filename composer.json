{
    "name": "akramzerarka/llama-cpp-php",
    "description": "The package enables the use of the LLama C++ library in PHP, thereby allowing the setup and execution of LLM models in PHP on your local machine.",
    "keywords": [
        "Kambo",
        "llama-cpp-php"
    ],
    "homepage": "https://github.com/kambo/llama-cpp-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Bohuslav Å imek",
            "email": "bohuslav@simek.si",
            "role": "Developer"
        },
        {
            "name": "Akram Zerarka",
            "email": "akram.zerarka@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/event-dispatcher": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^8.8"
    },
    "autoload": {
        "psr-4": {
            "Kambo\\LLamaCPP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kambo\\LLamaCPP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit",
        "format": "vendor/bin/phpcbf",
        "lint": "vendor/bin/phpcs"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
